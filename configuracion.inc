
configuracion:
    BANKSEL	TRISA
    CLRF	TRISB
    CLRF	TRISB 
    CLRF	TRISC
    CLRF	TRISD
    ; configurando TRISB como salida
    MOVLW	0XFF
    MOVWF	TRISA
    MOVLW	0X01
    MOVWF	TRISE
    
    BANKSEL	ANSELH      
    clrf	ANSEL             ; PUERTO CONFIGURADO COMO DIGITAL I/0 PORTA
    clrF	ANSELH            ; PUERTO CONFIGURADO COMO  DIGITAL I/0 ´PORTB

    BANKSEL	PORTB       ;   ACCESO AL BANCO 0, BIT 7 EN BAJO DEL REGISTRO STATUS
    CLRF	PORTB
    CLRF	PORTA
    CLRF	PORTC
    CLRF	PORTD
    CLRF	PORTE
    return
    
OSCILLATOR:
    BANKSEL  OSCCON
    MOVLW    0XE1
    MOVWF    OSCCON
    RETURN